#include <string>
#include <vector>

using namespace std;

string solution(vector<int> food) {
    string answer = "";
    for(int i = 1; i < food.size(); ++i) {
        for(int k = 0; k < food[i]/2; ++k)
            answer+=to_string(i);
    }
    answer+="0";
    for(int j = food.size()-1; j >= 0; --j) {
        for(int p = 0; p < food[j]/2; ++p)
            answer+=to_string(j);
    }
    return answer;
}
