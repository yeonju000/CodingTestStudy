#include <string>
#include <vector>
#include <algorithm>

using namespace std;

vector<string> solution(int n, vector<int> arr1, vector<int> arr2) {
    vector<string> answer;
    string a;
    int tmp;
    
    for(int i = 0 ; i < n;i++)
    {
        tmp = arr1[i] | arr2[i]; 
        
        while ( tmp != 0)
        {
            if( tmp %2 == 1) 
                a += "#";
            else 
                a +=" ";
            tmp/=2;
        }
        
        if ( a.size() != n)
        for(int j = a.size(); j < n;j++)
            a+=" ";
            
        reverse(a.begin(),a.end()); 
        
        answer.push_back(a);
        a = "";
    }
    
    return answer;
}
