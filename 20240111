#include string
#include vector

using namespace std;

int solution(string t, string p) {
    int answer = 0;
    int length = p.length();

    int tSize = static_castint(t.size());
    for (int i = 0; i  tSize - length + 1; ++i) {
        if (t.compare(i, length, p) = 0) {
            answer += 1;
        }
    }
    return answer;
}
